home(){
        echo "at home"
        mv ~/.m2/settings.xml ~/.m2/settings_home.xml
        echo "renamed settings.xml file"
}

office(){
        echo "at office"
        mv ~/.m2/settings_home.xml ~/.m2/settings.xml
        echo "renamed settings.xml file"
}

alias pretty='python -m json.tool'

token(){
        echo "getting token"
        curl https://171-ZQM-109.mktorest.com/identity/oauth/token\?grant_type=client_credentials\&client_id=3c47e4f4-58b9-4058-944e-872eb883e113\&client_secret=EHUHZi4drfXhqlaH1CUlbwcMrLWQNvxj | pretty | grep "access_token"
}

tokenSandbox(){
