        curl https://412-JIH-802.mktorest.com/identity/oauth/token\?grant_type=client_credentials\&client_id=8d9f485a-700e-4c0c-be95-f7207af10e74\&client_secret=V8AfenhgZZrwvGCEvB656EU79Af0WlZz | pretty | grep "access_token"
}

toggleWifi(){
	networksetup -getairportpower en0 | grep “On” && networksetup -setairportpower en0 off || networksetup -setairportpower en0 on
}

tidal(){
	cd "/Users/jmodi/Documents/Tidal/TES Java Client/" && ./tesclient.sh
}

